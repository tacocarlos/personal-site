generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("POSTGRES_PRISMA_URL")
}

model Project {
    id String @default(uuid()) @id
    name String @unique
    tags String[]
    summary String
    description String?

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    visible Boolean @default(false)
}